                                                <%= image_tag "tabmenu/tacos/#{item["image-name"]}.jpeg", :alt => "#{item["image-name"]}", :class => "img-responsive", :width => 100, :height => 100 %>
                                            </a>
                                            <div class="media-body">
                                                <h6><%= item["item"] %></h6>
                                                <span>$<%= item["price"] %></span>
                                            </div>
                                            <ul class="menu-in">
                                            <% item["ingredients"].split(",").each do |ingredient| %>
                                                <li><%= ingredient %></li>
                                            <% end %>
                                            </ul>
                                        </div>
                                    </li>
                                    <% end %>
                                <% end %>
