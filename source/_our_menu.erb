                <%= image_tag 'tabmenu/1_bottom.png'  %>
                <div class="bg-tabs" id="menu">
                    <div class="title-section style1 martp-0px">
                        <h1 class="title marbt-33px" >Our Menu</h1>
                    </div>
                    
                    <ul class="menu-tab">
                        <li class="active"><a href="#">Tacos   </a></li>
