<table class="table table-bordered">
  <tr>
      <th>Day</th>
      <th>Time</th>
      <th>Location</th>
  </tr>
  <% data.schedule.each do |day| %>
    <tr>
        <td><%= day["day"] %></td>
        <td><%= day["time"] %></td>
        <td><%= day["location"] %></td>
    </tr>
  <% end %>
</table>
