<table class="table table-bordered">
  <tr>
      <th>Day</th>
      <th>Time</th>
      <th>Location</th>
  </tr>
  <% sorted_data = data.schedule.sort_by { |h| h['order'].to_i } %>
  <% sorted_data.each do |day| %>
    <tr>
        <td><%= day["day"] %></td>
        <td><%= day["time"] %></td>
        <td><%= day["location"] %></td>
    </tr>
  <% end %>
</table>
